{
	"format_version": "1.10.0",
	"animation_controllers": {
		"controller.animation.pirate_ship": {
			"initial_state": "default",
			"states": {
				"default": {
					"transitions": [
						{
							"cancel_animation": "!query.is_alive"
						}
					]
				},
				"cancel_animation": {
					"animations": [
						"ship_death"
					],
					"transitions": [
						{
							"default": "query.is_alive"
						}
					]
				}
			}
		}
	}
}